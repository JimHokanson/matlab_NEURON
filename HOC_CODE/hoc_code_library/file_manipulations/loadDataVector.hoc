proc loadDataVector(){localobj file_ref, temp_save_vec
// 
//	JIM NOTE: localobj must be on first line
//
//	loadDataVector("name","inputs/fileName",vector_obj)
//
//
// IMPROVEMENTS: prepend data to input instead of making input pass it in ...

	file_ref      = new File()
	temp_save_vec = new Vector()
	
	file_ref.ropen($s1)
	temp_save_vec = $o2
	temp_save_vec.vread(file_ref,4)
	file_ref.close()
}